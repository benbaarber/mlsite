{
  "name": "mlsite",
  "version": "1.0.0",
  "description": "mobile list website",
  "main": "index.js",
  "repository": "git@github.com:benbaarber/mlsite.git",
  "author": "Zoink Doink",
  "license": "MIT",
  "scripts": {
    "build:server": "webpack --config server/webpack.config.js",
    "build:client": "npx tailwindcss -c ./client/tailwind.config.js -i ./client/src/style.css -o ./dist/client/style.css --minify && webpack --config client/webpack.config.js",
    "build": "yarn build:server && yarn build:client",
    "start": "node dist/server/bundle.js",
    "dev": "nodemon --config client/nodemon.json",
    "format:server": "npx prettier server/src --write && npx prettier server/test --write",
    "format:client": "prettier --write 'client/src/**/*.{ts,tsx,css}' --config ./client/.prettierrc",
    "format": "yarn format:server && yarn format:client",
    "test:db": "jest ./server --env=node --detectOpenHandles -i --verbose"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/bootstrap": "^5.2.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.9",
    "@types/react-bootstrap": "^0.32.32",
    "@types/react-dom": "^18.0.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "autoprefixer": "^10.4.13",
    "css-loader": "^6.7.2",
    "daisyui": "^2.46.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "https": "^1.0.0",
    "jest": "^29.3.1",
    "jsdom": "^20.0.3",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-tailwindcss-select": "^1.6.0",
    "redux": "^4.2.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.7.3",
    "mongoose-lean-virtuals": "^0.9.1",
    "prettier": "^2.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
